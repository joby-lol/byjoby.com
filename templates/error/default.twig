{% extends 'main.twig' %}

{% block title %}Error{% endblock %}

{% block body %}
    <section class="main-page">
        <h1>An unknown error occurred</h1>
        <p>Sorry, the server was unable to process your request.</p>
        <p>Exception type: {{ eclass }}</p>
        <p>Return code: {{ code }}</p>
    </section>
{% endblock %}